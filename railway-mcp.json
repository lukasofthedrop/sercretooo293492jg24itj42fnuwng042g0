{
  "name": "lucrativabet-railway-mcp",
  "version": "1.0.0",
  "description": "Railway MCP integration for LucrativaBet Casino Application",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "./deploy-railway.sh",
    "status": "railway status",
    "logs": "railway logs",
    "env": "railway variables",
    "restart": "railway restart"
  },
  "dependencies": {
    "@railway/cli": "^3.0.0",
    "express": "^4.18.0",
    "axios": "^1.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "railway",
    "mcp",
    "lucrativabet",
    "casino",
    "laravel",
    "deployment"
  ],
  "author": "LucrativaBet Team",
  "license": "MIT",
  "railway": {
    "build": {
      "command": "npm install"
    },
    "deploy": {
      "startCommand": "npm start"
    },
    "env": {
      "NODE_ENV": "production",
      "RAILWAY_TOKEN": "${{ secrets.RAILWAY_TOKEN }}",
      "RAILWAY_PROJECT_ID": "${{ secrets.RAILWAY_PROJECT_ID }}"
    }
  }
}